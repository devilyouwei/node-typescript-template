    "compilerOptions": {
        "target": "esnext",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "module": "commonjs",
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": false,
        "sourceMap": false,
        "rootDir": "./",
        "outDir": "./dist",
        "esModuleInterop": true
    }
}
